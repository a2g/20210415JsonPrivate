{
    "$id": "https://antcassidy.com/mansion.objects",
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "a list of specific objects, and the reactions you can use on them",
    "type": "object",
    "properties": {
        "objectivePropName": {
            "$ref": "objects/Objects.json#/definitions/prop_type"
        },
        "reactions": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/transaction_type"
            }
        },
        "startingInvs": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/inv_wrapper_type"
            }
        },
        "startingProps": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/prop_wrapper_type"
            }
        },
        "startingPropsArray": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "objects/Objects.json#/definitions/prop_type"
            }
        }
    },
    "definitions": {

        "inv_wrapper_type": {
            "type": "object",
            "required": [ "inv" ],
            "properties": {
                "inv": {
                    "$ref": "objects/Objects.json#/definitions/inv_type"
                }
            }
        },
        "prop_wrapper_type": {
            "type": "object",
            "required": [ "prop" ],
            "properties": {
                "prop": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                }
            }
        },
        "transaction_type": {
            "type": "object",
            "required": [ "type" ],
            "properties": {
                "type": {
                    "$ref": "https://raw.githubusercontent.com/a2g/adv/main/adv.schema.json#/the_enum"
                },
                "reg1": {
                    "$ref": "objects/Objects.json#/definitions/reg_type"
                },
                "reg2": {
                    "$ref": "objects/Objects.json#/definitions/reg_type"
                },
                "inv1": {
                    "$ref": "objects/Objects.json#/definitions/inv_type"
                },
                "inv2": {
                    "$ref": "objects/Objects.json#/definitions/inv_type"
                },
                "inv3": {
                    "$ref": "objects/Objects.json#/definitions/inv_type"
                },
                "prop1": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                },
                "prop2": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                },
                "prop3": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                },
                "prop4": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                },
                "prop5": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                },
                "prop6": {
                    "$ref": "objects/Objects.json#/definitions/prop_type"
                }
            },
            {
                "$ref": "https://raw.githubusercontent.com/a2g/adv/main/adv.schema.json#/big_set_of_if_thens"
            }
            
        }
    },
}
